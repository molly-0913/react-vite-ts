{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode fat --host",
    "start": "npm run dev",
    "build:fat": "tsc --noEmit && vite build --mode fat",
    "build:uat": "tsc --noEmit && vite build --mode uat",
    "build:prod": "tsc --noEmit && vite build --mode prod",
    "build": "npm run build:fat && npm run build:uat && npm run build:prod",
    "lint": "eslint src --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "antd-mobile": "^5.38.1",
    "axios": "^1.7.7",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "encryptlong": "^3.1.4",
    "jsrsasign": "^11.1.0",
    "nanoid": "^5.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.28.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jsrsasign": "^10.5.14",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "sass": "^1.81.0",
    "tailwindcss": "^3.4.15",
    "terser": "^5.36.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10"
  }
}
